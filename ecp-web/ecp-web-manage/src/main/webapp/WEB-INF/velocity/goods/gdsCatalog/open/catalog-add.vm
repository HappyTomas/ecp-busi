<script type="text/javascript" src="$!{BUSIJS_PATH}/goods/gdsCatalog/catalog-add.js?version=$!{JS_VERSION}" ></script>


			
			<form id="detailInfoForm" class="form-horizontal  required-validate" style="margin-bottom: 0px;" method="post">
				<div class="row-fluid">
					<div class="span4">
						<div class="control-group" style="width:400px;">
							<label  class="control-label">目录编码：</label>
							<div class="controls">
								<input type="text" id="catlogCode" name="catlogCode" class="input-large htmlcheck digits required"  maxlength="16" placeholder="输入目录编码" />
							</div>
						</div>
                    </div>
                </div>
				<div class="row-fluid">
					<div class="span4">
						<div class="control-group" style="width:400px;">
							<label  class="control-label">目录名称：</label>
							<div class="controls">
								<input type="text" id="catlogName" name="catlogName" class="input-large required htmlcheck" maxlength="64" placeholder="输入目录名称"/>
							</div>
						</div>
                    </div>
                </div>
	
				<div class="row-fluid">
					<div class="span4">
						<div class="control-group" style="width:400px;">
							<label for="" class="control-label">目录描述：</label>
							<div class="controls">
								
								<textarea rows="5" class="input-large htmlcheck" maxlength="100" name="catlogDesc" id="catlogDesc" placeholder="输入目录描述"></textarea>
							</div>
						</div>
					</div>
				</div>
                           
			</form>
				<div style="text-align: center;margin-top:30px fbtn">
					<button type="button" class="btn btn-info" style="margin-right:10px" id="btnFormSave"><i class="icon-ok-sign icon-white"></i> 保　存</button>
					<button type="button" class="btn" id="btnFormCancle"><i class="icon-remove-sign icon-white"></i> 取　消</button>
				</div>
                
				
      